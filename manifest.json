{
  "author": "Shraymonks",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "description": "Automatically redirect to AmazonSmile from Amazon",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "AmazonSmile Auto Redirect",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.amazon.com/*"
  ],
  "version": "1.0.0"
}
